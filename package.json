{
  "name": "chartjs-plugin-stanford-diagram",
  "description": "Stanford Diagram plugin for Chart.js",
  "author": "Daniel Oliveira <daniel.oliveira@present-technologies.com>",
  "version": "2.0.0-beta",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/chartjs-plugin-stanford-diagram.esm.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "chart",
    "chartjs",
    "chart.js",
    "stanford",
    "diagram",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danieloliveira117/chartjs-plugin-stanford-diagram.git"
  },
  "bugs": {
    "url": "https://github.com/danieloliveira117/chartjs-plugin-stanford-diagram/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "run-s build demo",
    "watch": "tsdx watch --no-clean",
    "demo": "cd demo && npm start",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint src test --ignore-pattern src/stanford-utils.ts",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test"
    }
  },
  "dependencies": {
    "chart.js": "^3.0.0-beta.8"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.7",
    "jest-canvas-mock": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
